# Application
PORT=5000
NODE_ENV=staging

# MongoDB
MONGO_URI=mongodb://mongodb:27017/dailyspending
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=your_secure_staging_password

# Authentication
JWT_SECRET=your_staging_jwt_secret_here

# Anthropic API
ANTHROPIC_API_KEY=your_staging_api_key

# Logging
LOG_LEVEL=info

# GitHub Container Registry
GITHUB_REPOSITORY=yourusername/yourrepo
IMAGE_TAG=latest